%tr
  %td= h user
  %td= user.email
  %td= user.role
  %td
    = link_to image_tag('icon_small_show.png', :border => 0), admin_user_path(user)
    = link_to image_tag('icon_small_edit.png', :border => 0), edit_admin_user_path(user)
    = link_to image_tag('icon_small_delete.png', :border => 0),admin_user_path(user), :confirm => 'Are you sure?', :method => :delete